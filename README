COMPILE (normal)
make all

COMPILE (for valgrind)
make for_valgrind

VALGRIND memcheck
valgrind --leak-check=yes ./build/ValOut

VALGRIND massif
valgrind --tool=massif --massif-out-file=valgrind/massif.out ./build/ValOut

* for short programs add --time-unit=B

VALGRIND massif print results
ms_print valgrind/massif.out